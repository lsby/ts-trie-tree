{
  "name": "@lsby/trie-tree",
  "version": "0.0.7",
  "description": "一个 ts 实现的字典树.",
  "author": "hbybyyang",
  "main": "dist/src/Index.js",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "=====build=====": "",
    "build:ts": "tsc",
    "=====watch=====": "",
    "watch:ts": "tsc -w",
    "=====test=====": "",
    "test": "cross-env NODE_ENV=dev mocha dist/test/index.test.js --bail --timeout 99999999",
    "coverage": "cross-env NODE_ENV=dev nyc --clean --reporter=html --reporter=text-summary npm run test && open-cli ./coverage/index.html",
    "=====npm=====": "",
    "prepub": "npm i && npm run build:ts",
    "pub": "npm publish --access=public",
    "=====其他=====": "",
    "format:all": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "open-cli": "^7.1.0",
    "prettier": "^3.7.4",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lsby/ts-trie-tree.git"
  }
}
